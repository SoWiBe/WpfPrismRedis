<UserControl x:Class="WprPrismRedis.Views.ToolBar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WprPrismRedis.Views"
        mc:Ignorable="d"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True">
    <StackPanel>
        <TextBlock Text="This is ToolBar" HorizontalAlignment="Center"
                   FontWeight="Bold"/>
        <StackPanel Orientation="Horizontal">
            <Button Content="Navigate to Logger" Margin="5"
                    Command="{Binding NavigateCommand}" Width="150"/>
        </StackPanel>
    </StackPanel>
</UserControl>
